<p id="notice"><%= notice %></p>

<h1>Listing Product Related Products</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Related prooduct</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_related_products.each do |product_related_product| %>
      <tr>
        <td><%= product_related_product.product_id %></td>
        <td><%= product_related_product.related_prooduct %></td>
        <td><%= link_to 'Show', product_related_product %></td>
        <td><%= link_to 'Edit', edit_product_related_product_path(product_related_product) %></td>
        <td><%= link_to 'Destroy', product_related_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product related product', new_product_related_product_path %>
